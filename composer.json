{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "aristridely/slideover": "^1.0",
        "biscolab/laravel-recaptcha": "^5.4",
        "blade-ui-kit/blade-heroicons": "^2.0",
        "blockpc/select2-wire": "^1.1",
        "doctrine/dbal": "^3.5",
        "guzzlehttp/guzzle": "^7.5",
        "jamesmills/laravel-timezone": "^1.11",
        "jenssegers/agent": "^2.6",
        "laracraft-tech/laravel-date-scopes": "^1.0",
        "laravel/framework": "^9.19",
        "laravel/jetstream": "^2.12",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^10.0",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.12",
        "lwwcas/laravel-countries": "^3.3",
        "mallardduck/blade-lucide-icons": "^1.10",
        "php-flasher/flasher-laravel": "^1.13",
        "power-components/livewire-powergrid": "^3.6",
        "rappasoft/laravel-authentication-log": "^2.0",
        "rappasoft/laravel-livewire-tables": "^2.9",
        "rawilk/laravel-form-components": "^8.1",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.7",
        "spatie/laravel-sitemap": "^6.2",
        "torann/geoip": "^3.0",
        "unicodeveloper/laravel-paystack": "^1.1",
        "unisharp/laravel-filemanager": "^2.5",
        "wire-elements/modal": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "treeware/plant": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
